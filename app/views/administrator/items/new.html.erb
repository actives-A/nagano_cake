<h1>商品新規登録</h1>
<%= form_with model: @list ,method: :get do |f| %>
<%= f.label :image %>
<%= f.file_field :image %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :explanation %>
<%= f.text_area :explanation %>

<%= f.label "ジャンル" %>
<%= f.select :genre_id,[1,2] %>

<%= f.label "税抜き価格" %>
<%= f.text_field :out_tax_price %>

<% end %>
